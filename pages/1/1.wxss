/* pages/1/1.wxss */
.container {
  background: #7acfa6;
  align-items: stretch;
  padding: 0;
  height: 100%;
  overflow: hidden;
}
.help {
  position: absolute;
  z-index: 100;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
}


.help-q {
  color: #999;
  font-size: 11pt;
  line-height: 200%;
  margin-top: 5rpx;
}

.help-a {
  text-indent: 1em;
  line-height: 160%;
  display: flex;
  flex-direction: column;
}

.help-a text {
  word-break: break-all;
}

